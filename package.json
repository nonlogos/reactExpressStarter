{
  "name": "starter-with-react",
  "version": "0.0.1",
  "description": "starter app with node server, client in react",
  "repository": {
    "type": "git",
    "url": "github.com/nonlogs"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "webpack": "./node_modules/.bin/babel-node ./node_modules/.bin/webpack",
    "watch:api": "./node_modules/.bin/nodemon -e js --watch --exec ./node_modules/.bin/babel-node server/api-server/index.js",
    "watch:ui": "yarn run webpack --mode development --config webpack_dev.config.js",
    "watch": "concurrently --names 'API,UI' -c 'bgBlue,bgMagenta' 'yarn watch:api' 'yarn watch:ui'",
    "start:api": "./node_modules/.bin/babel-node server/api-server/index.js",
    "start:ui": "./node_modules/.bin/babel-node server/asset-server/index.js",
    "start": "concurrently --names 'API, UI' -c 'bgBlue, bgMagento' 'yarn run start:api' 'yarn run start:ui'",
    "build:dev": "yarn clean && yarn webpack --config webpack.config.js --env dev",
    "build:prod": "yarn clean && yarn webpack --config webpack.config.js --env prod",
    "clean": "rm -rf dist/* && yarn run prepcerts",
    "test": "jest",
    "prepvapid": "web-push generate-vapid-keys --json > ./private/vapid.json",
    "prepcerts": "yarn run prepvapid && ./node_modules/.bin/babel-node ./server/gen-certs.js && rm -rf dist/*.pem && mkdir -p dist && cp private/*.pem dist/",
    "sementic-release": "sementic-release",
    "travis-deploy-once": "travis-deploy-once",
    "serve": "./node_modules/.bin/http-serve ./dist -g -S -C ./dist/cert.pem -K ./dist/key.pem -o"
  },
  "author": "Connie Yu",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "devcert-with-localhost": "^0.3.3",
    "express": "^4.16.4",
    "idb": "^2.1.3",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "4.16.0",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/webpack-hot-middleware": "^2.16.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.11.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-serve": "^1.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "semantic-release": "^15.13.2",
    "style-ext-html-webpack-plugin": "^4.0.1",
    "style-loader": "^0.23.1",
    "travis-deploy-once": "^5.0.11",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "worker-loader": "^2.0.0"
  }
}
